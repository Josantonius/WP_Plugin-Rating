        "Josantonius/WP_Plugin-Info"   : "^1.0.0",
        "composer/installers"          : "^1.3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Modules\\WP_Plugin_Rating\\" : "src/"
        }
    }
}