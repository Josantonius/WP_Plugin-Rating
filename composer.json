{
    "name":        "josantonius/wp-plugin-rating",
    "version":     "1.0.0",
    "description": "Show plugin rating in WordPress administration pages for plugins developed with Eliasis Framework.",
    "type": "eliasis-module",
    "keywords": [
        "Wordpress", 
        "Plugin", 
        "Eliasis",
        "Component",
        "Rating",
        "PHP",
        "HHVM"
    ],
    "license": "GPL-2.0+",
    "authors": [
        {
            "name":     "Josantonius",
            "email":    "hello@josantonius.com",
            "homepage": "https://josantonius.com",
            "role":     "Developer"
        }
    ],
    "config": {
        "vendor-dir": "lib/vendor",
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/Josantonius/wp-plugin-rating/issues",
        "forum": "http://stackoverflow.com/tags/wp-plugin-rating",
        "source": "https://github.com/Josantonius/wp-plugin-rating"
    },
    "require": {
        "php": "^5.3 || ^7.0",
        "eliasis-framework/eliasis"    : "^1.0.7",
        "Josantonius/WP_Register"      : "^1.0.0",
